{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": [
    "import"
  ],
  "root": true,
  "rules": {
    "quotes": ["error", "single", {
      "allowTemplateLiterals": true
    }]
  }
}
