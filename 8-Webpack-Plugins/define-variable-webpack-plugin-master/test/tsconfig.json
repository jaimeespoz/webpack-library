{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "noImplicitAny": false,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "lib": [
      "dom",
      "esnext"
    ]
  }
}